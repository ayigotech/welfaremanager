{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "www",
    "installCommand": "npm install",
    "devCommand": "ionic serve",
    "framework": "angular",
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/index.html"
    }]
}